# URL to the backend's index page
BACKEND_URL=http://localhost:3000

# JWT secret
JWT_KEY=jwtkey

# JWT time to expiry
JWT_EXPIRES_IN=2h

# Your Mongodb Atlas URI
MONGO_URI=mongodb+srv://username:password@yourdatabase.mongodb.net/?retryWrites=true&w=majority

# Current base url to newsapi
NEWSAPI_BASE_URL=https://newsapi.org/v2

# API key to https://newsapi.org/
NEWSAPI_KEY=yournewsapikey

# AWS
AWS_REGION=eu-central-1
AWS_ACCESS_KEY_ID=accesskeyid
AWS_SECRET_ACCESS_KEY=secretaccesskey
AWS_PUBLIC_BUCKET_NAME=cobe-backend